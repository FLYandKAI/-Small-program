/* miniprogram/pages/acupoint/acupoint.wxss */
.background{
  width: 100%;
  height: 150rpx;
}
.acupoints{
  position: relative;
  margin-top: 20rpx;
}
.acupoints .acupoint-item{
  border: 1rpx solid #bcbcbd;
  display: inline-block;
  width: 250rpx;
  height: 100rpx;
  margin-left: 10%;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  text-align: center;
}
.acupoints .acupoint-item text{
  display: inline-block;
  font-size: 40rpx;
  margin-top: 20rpx;
  color: #686767;
}
.arrow{
  float: right;
  margin-top: 20rpx;
  width: 29px;
  height: 29px;
}
